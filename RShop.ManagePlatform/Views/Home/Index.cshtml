@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title</title>
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="//cdn.bootcss.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="//cdn.bootcss.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href="~/js/plugins/select2/select2.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/css/AdminLTE.css" rel="stylesheet" />
    <link href="~/css/skins/skin-blue.css" rel="stylesheet" />
    <link href="~/css/app.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="//cdn.bootcss.com/html5shiv/r29/html5.min.js"></script>
    <script src="//cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style>
        html, body, .wrapper, .content-wrapper {
            height: 100%;
            overflow: hidden;
        }
    </style>

    <!-- REQUIRED JS SCRIPTS 此处JS 放iframe 之后不加载，原因未知-->
    <script src="~/js/plugins/jQuery/jQuery-2.2.0.min.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/plugins/artDialog/jquery.artDialog.js?skin=simple"></script>
    <script src="~/js/plugins/artDialog/plugins/iframeTools.source.js"></script>
    <script src="~/js/plugins/select2/select2.full.js"></script>
    <script src="~/js/AdminLTE.App.js"></script>
    <script src="~/js/app.js"></script>

</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>I4TA网</b>平台运维</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>I4TA网</b>平台运维</span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <li class="dropdown messages-menu">
                            <!-- Menu toggle button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-envelope-o"></i>
                                <span class="label label-success">4</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have 4 messages</li>
                                <li>
                                    <!-- inner menu: contains the messages -->
                                    <ul class="menu">
                                        <li>
                                            <!-- start message -->
                                            <a href="#">
                                                <div class="pull-left">
                                                    <!-- User Image -->
                                                    <img src="" class="img-circle" alt="User Image">
                                                </div>
                                                <!-- Message title and timestamp -->
                                                <h4>
                                                    Support Team
                                                    <small><i class="fa fa-clock-o"></i> 5 mins</small>
                                                </h4>
                                                <!-- The message -->
                                                <p>Why not buy a new awesome theme?</p>
                                            </a>
                                        </li>
                                        <!-- end message -->
                                    </ul>
                                    <!-- /.menu -->
                                </li>
                                <li class="footer"><a href="#">See All Messages</a></li>
                            </ul>
                        </li>
                        <!-- /.messages-menu -->
                        <!-- Notifications Menu -->
                        <li class="dropdown notifications-menu">
                            <!-- Menu toggle button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning">10</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have 10 notifications</li>
                                <li>
                                    <!-- Inner Menu: contains the notifications -->
                                    <ul class="menu">
                                        <li>
                                            <!-- start notification -->
                                            <a href="#">
                                                <i class="fa fa-users text-aqua"></i> 5 new members joined today
                                            </a>
                                        </li>
                                        <!-- end notification -->
                                    </ul>
                                </li>
                                <li class="footer"><a href="#">View all</a></li>
                            </ul>
                        </li>
                        <!-- Tasks Menu -->
                        <li class="dropdown tasks-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger">9</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">You have 9 tasks</li>
                                <li>
                                    <!-- Inner menu: contains the tasks -->
                                    <ul class="menu">
                                        <li>
                                            <!-- Task item -->
                                            <a href="#">
                                                <!-- Task title and progress text -->
                                                <h3>
                                                    Design some buttons
                                                    <small class="pull-right">20%</small>
                                                </h3>
                                                <!-- The progress bar -->
                                                <div class="progress xs">
                                                    <!-- Change the css width attribute to simulate progress -->
                                                    <div class="progress-bar progress-bar-aqua" style="width: 20%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                                        <span class="sr-only">20% Complete</span>
                                                    </div>
                                                </div>
                                            </a>
                                        </li>
                                        <!-- end task item -->
                                    </ul>
                                </li>
                                <li class="footer">
                                    <a href="#">View all tasks</a>
                                </li>
                            </ul>
                        </li>
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                <span class="hidden-xs">Rocher</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- The user image in the menu -->
                                <li class="user-header">
                                    <p>
                                        Alexander Pierce - Web Developer
                                        <small>Member since Nov. 2012</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <li class="user-body">
                                    <div class="row">
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Followers</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Sales</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">Friends</a>
                                        </div>
                                    </div>
                                    <!-- /.row -->
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">Profile</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                    <li class="header">主菜单</li>
                    <!-- Optionally, you can add icons to the links -->
                    <li class="active"><a href="/"><i class="fa fa-dashboard"></i> <span> 仪表盘</span></a></li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-black-tie"></i> <span>会员管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="/Member/" target="mainContent"><i class="fa fa-circle-o"></i>个人账户列表</a></li>
                            <li><a href="/Enterprise/" target="mainContent"><i class="fa fa-circle-o"></i>企业账户列表</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>专属顾问</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>售后服务</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>更换顾问申请</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-get-pocket"></i> <span>供方管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>添加账号</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>账号列表</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>供方文件</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-table"></i> <span>产品管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="/ProductCategory" target="mainContent"><i class="fa fa-circle-o"></i>产品分类</a></li>
                            <li><a href="/ProductAttribute" target="mainContent"><i class="fa fa-circle-o"></i>公共参数管理</a></li>
                            <li><a href="/Brand" target="mainContent"><i class="fa fa-circle-o"></i>品牌管理</a></li>
                            <li><a href="/Product" target="mainContent"><i class="fa fa-circle-o"></i>产品列表</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-hand-scissors-o"></i> <span>询价管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>待处理</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>已处理</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-hand-paper-o"></i> <span>议价管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>待处理</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>已处理</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-heart"></i> <span>订单管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>待确认</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>已确认</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-balance-scale"></i> <span>框架合同管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>待审核</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>执行中</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>框架订单</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-graduation-cap"></i> <span>市场运维</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>站内信</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>公告</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>Banner</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-user-md"></i> <span>顾问管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>顾问列表</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>客户咨询</a></li>
                            <li><a href="#" target="mainContent"><i class="fa fa-circle-o"></i>评分管理</a></li>
                        </ul>
                    </li>

                    <li class="treeview">
                        <a href="#"><i class="fa  fa-line-chart"></i> <span>统计报表</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="/Report" target="mainContent"><i class="fa fa-circle-o"></i>产品销售量</a></li>
                            <li><a href="/Report" target="mainContent"><i class="fa fa-circle-o"></i>成交额</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#"><i class="fa fa-cogs"></i> <span>系统管理</span> <i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="treeview-menu">
                            <li><a href="/Role" target="mainContent"><i class="fa fa-circle-o"></i>角色权限管理</a></li>
                            <li><a href="/Administrator" target="mainContent"><i class="fa fa-circle-o"></i>管理员管理</a></li>
                        </ul>
                    </li>
                </ul>
                <!-- /.sidebar-menu -->
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <iframe id="mainContent" name="mainContent" src="/Home/Dashboard" style="width:100%;height:100%;" frameborder="0" scrolling="auto" />
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- ./wrapper -->
</body>

</html>
